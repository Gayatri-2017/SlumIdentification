https://anitagraser.com/2010/11/30/creating-high-resolution-images-using-qgis/

qgis --project myproject.qgs --snapshot image.png --width 1500 --height 1000 --extent xmin,ymin,xmax,ymax

qgis --project GoogleSatellite.qgs --snapshot image.png --width 1500 --height 1000 --extent 8109284.3,2164982.0,8110150.7,2164480.4

qgis --project GoogleSatellite.qgs --snapshot image.png --width 1500 --height 1000 --extent 8113416,2165891,8114476,2165264

